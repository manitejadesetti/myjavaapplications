<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
			http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
			http://www.springframework.org/schema/context 
			http://www.springframework.org/schema/context/spring-context-4.0.xsd">
<bean id="emp" class="core.Employee" autowire="byName">
<property name="code" value="101"></property>
<property name="name" value="abcde"></property>
   
</bean>
<bean id="address" class="core.Address">
<property name="street" value="Madhapur"></property>
<property name="city" value="Hyderabad"></property>
</bean>
<bean id="student" class="core.Student">
       <constructor-arg name="regno" value="100"></constructor-arg> 
       <constructor-arg name="name" value="Mani"></constructor-arg>
       <constructor-arg name="marks" value="90"></constructor-arg> 
</bean>
	
</beans>